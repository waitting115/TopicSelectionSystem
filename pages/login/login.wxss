.all {
  width: 100%;
  height: 100%;
  background: #f2f2f2;
  background-size: 100% 100%;
  padding: 0;
  margin: 0;
}
.title1, .title2 {
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  align-items: center;
}
.title1 {
  width: 100%;
  height: 200rpx;
  font-size: 70rpx;
}
.title2 {
  width: 100%;
  height: 100rpx;
  font-size: 60rpx;
  margin-bottom: 40rpx;
}
.loginModel {
  border: 1px solid #ccc;
  border-radius: 20rpx;
}
.userView, .passwordView {
  /* width: 90%; */
  display: flex;
  flex-flow:row nowrap;
  justify-content: space-around;
  align-items: center;

  margin: 40rpx 0;
}
.userLabel, .passwordLabel {
  width: 20%;
  /* background: red; */
  font-size: 50rpx;
}
.user, .password {
  background: #fff;
  width: 60%;
  height: 70rpx;
  border: 1px solid #ccc;
  border-radius: 10rpx;
}
.submitView {
  width: 100%;
  height: 200rpx;

  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}