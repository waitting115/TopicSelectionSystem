.all {
  width: 100%;
  height: 100%;
  background: #f2f2f2;
  padding: 0;
  margin: 0;
}
.title {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}
.title view {
  font-size: 50rpx;
  font-family: kaiti;
}
.content {
  width: 100%;
  /* height:  */
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  align-items: center;
}
.item {
  width: 90%;
  height: 80rpx;
  border: 1px solid #ccc;
  border-radius: 20rpx;
  background: #fff;
  margin: 10rpx 0;

  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
  align-items: center;
}
